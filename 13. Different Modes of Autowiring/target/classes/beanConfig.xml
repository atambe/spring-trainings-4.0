<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="foo" class="com.cg.demo.Foo" ></bean>
	<bean id="bar1" class="com.cg.demo.Bar"></bean>
	
	<bean id="targetByName" class="com.cg.demo.Target" autowire="byName" lazy-init="true"></bean>
	<bean id="targetByType" class="com.cg.demo.Target" autowire="byType" lazy-init="true"></bean>
	<bean id="targetConstructor" class="com.cg.demo.Target" autowire="constructor" lazy-init="true"></bean>
</beans>
